{
  "name": "spn",
  "version": "5.6.2",
  "description": "Sprite Engine",
  "main": "spn.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "lint": "standard",
    "clean": "rimraf spn.* && rimraf coverage",
    "doc": "esdoc -c esdoc.json",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "dist": "rollup -f umd -n spn src/index.js | buble | tee spn.js | uglifyjs -m -c > spn.min.js",
    "prepublish": "npm run clean && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/spn.git"
  },
  "keywords": [
    "sprite",
    "dom",
    "animation",
    "grid"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kt3k/spn/issues"
  },
  "homepage": "https://github.com/kt3k/spn#readme",
  "peerDependencies": {
    "capsid": ">=0.3.0",
    "jquery": ">=2.0.0"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.2.0",
    "browserify": "^14.2.0",
    "buble": "^0.15.2",
    "capsid": "^0.7.0",
    "chai": "^4.0.1",
    "codecov.io": "^0.1.6",
    "dom-gen": "^2.1.0",
    "es6-promise": "^4.0.4",
    "esdoc": "^1.0.0-alpha.2",
    "jquery": "^3.0.0",
    "karma": "^1.0.0",
    "karma-browserify": "^5.0.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "mocha": "^3.0.0",
    "rimraf": "^2.5.1",
    "rollup": "^0.41.4",
    "standard": "^10.0.0",
    "uglify-js": "^3.0.0",
    "watchify": "^3.7.0"
  },
  "standard": {
    "env": [
      "browser",
      "mocha"
    ],
    "globals": [
      "expect",
      "$"
    ]
  }
}
